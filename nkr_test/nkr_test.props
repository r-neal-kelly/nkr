<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <nkr>$(SolutionDir)\nkr\include</nkr>
    <doctest>$(SolutionDir)\doctest\include</doctest>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="nkr">
      <Value>$(nkr)</Value>
    </BuildMacro>
    <BuildMacro Include="doctest">
      <Value>$(doctest)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>